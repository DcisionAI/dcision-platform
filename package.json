    "pdf-parse": "^1.1.1",
    "pdf-poppler": "^0.2.1",
    "pino-pretty": "^11.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.4",
    "react-markdown": "^9.0.1",
