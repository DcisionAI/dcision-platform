{
  "name": "dcisionai-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "create-demo-user": "ts-node scripts/create-demo-user.ts",
    "generate:openapi": "node scripts/generate-openapi.js",
    "generate:sdk": "npm run generate:openapi",
    "db:migrate": "supabase db push",
    "build:dev": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "build:prod": "npm run clean && NODE_OPTIONS='--max-old-space-size=4096' NODE_ENV=production next build",
    "build:analyze": "ANALYZE=true next build",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "clean:all": "npm run clean && rm -rf node_modules && npm install",
    "prebuild": "npm --prefix packages/sdk-js run generate",
    "verify:highs-mcp": "node scripts/verify-highs-mcp.js",
    "verify:single-service": "node scripts/verify-single-service.js",
    "start:highs": "npx highs-mcp",
    "start:dev": "concurrently \"npm run start:highs\" \"npm run dev\""
  },
  "dependencies": {
