{
  "name": "dcisionai-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "create-demo-user": "ts-node scripts/create-demo-user.ts",
    "generate:openapi": "node scripts/generate-openapi.js",
    "generate:sdk": "npm run generate:openapi",
    "db:migrate": "supabase db push",
    "build:dev": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "build:prod": "npm run clean && NODE_OPTIONS='--max-old-space-size=4096' NODE_ENV=production next build",
    "build:analyze": "ANALYZE=true next build",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "clean:all": "npm run clean && rm -rf node_modules && npm install",
    "prebuild": "npm --prefix packages/sdk-js run generate",
    "verify:highs-mcp": "node scripts/verify-highs-mcp.js",
    "verify:single-service": "node scripts/verify-single-service.js",
    "start:dev": "npm run dev",
    "deploy": "./scripts/deploy.sh",
    "deploy:platform": "./scripts/deploy-platform.sh",
    "deploy:solver": "./scripts/deploy-solver.sh",
    "build:solver": "docker build -f Dockerfile.solver -t solver-service .",
    "run:solver": "docker run -p 8081:8080 solver-service",
    "solver:local": "docker-compose -f docker-compose.solver.yml up",
    "solver:test": "curl -X POST http://localhost:8081/solve -H \"Content-Type: application/json\" -d '{\"problem\":{\"objective\":{\"sense\":\"minimize\",\"linear\":[1,1]},\"variables\":[{\"name\":\"x\",\"type\":\"cont\",\"lb\":0,\"ub\":10},{\"name\":\"y\",\"type\":\"cont\",\"lb\":0,\"ub\":10}],\"constraints\":[{\"coefficients\":[1,1],\"sense\":\">=\",\"rhs\":1}]},\"solver\":\"highs\"}'",
    "test:solver": "./scripts/test-solver-service.sh"
  },
